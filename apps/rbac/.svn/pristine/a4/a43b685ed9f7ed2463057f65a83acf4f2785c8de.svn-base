.skin-blue .navbar-top {background-color: #337ab7; color: #fff;}
.skin-blue .navbar-top .dropdown .dropdown-toggle {color: #fff;font-size: 16px;}
.skin-blue .navbar-top .dropdown .dropdown-toggle:hover, .skin-blue .navbar-top .dropdown .dropdown-toggle:focus {background-color: #2d6da2;}
.skin-blue .navbar-top .navbar-brand {background-color: #2d6da2; color: #fff;}
.navbar-top-links .active > a, .nav .open > a, .nav .open > a:focus, .nav .open > a:hover {background-color: #2d6da2;}
// 固定头部
.fixed .main-header {position: fixed;width: 100%;}
@media (min-width: 1919px) {
  .adaptive .navbar-top .navbar-brand { width: 181px; text-align: center;}
}
@media (max-width: 768px) {
  .skin-blue .navbar-header {
    display: none;
  }
  .skin-blue .navbar-right {
    display: none;
  }
}
@media (max-width: 1919px) {
  .adaptive .navbar-collapse {
    display: none;
  }
}
@media (min-width: 768px) {
  .modal-dialog {margin: 10% auto;}
}
// 头部
.btn-top {
  width: 40px;
  height: 40px;
  border-radius: 50%;
  background: #000;
  position: fixed;
  bottom: 10px;
  right: 10px;
  color: #fff;
  opacity: 0.5;
  display: none;
  -moz-transition: background 1s;
  -webkit-transition: background 1s;
  -o-transition: background 1s;
}
.dialog-content {padding: 20px;min-height:450px;}
.content {background: #fff;}
.content .panel {margin-bottom: 0px;}
.content .panel-btn .panel-heading {padding: 5px;}
.content .panel-body {padding: 5px 0px;}
.btn-top:active {border: none;}
.form-selectpicker {min-width: 160px;}
.navbar-brand small {font-size: 60%; font-style: italic}
.nav-curstom {margin-top: 10px;}
.nav-curstom > li.active > a, .nav-curstom > li.active > a:focus, .nav-curstom > li.active > a:hover {color: #fff; background-color: #337ab7; border-color: #337ab7;}
.pagination-detail {margin-right: 20px; padding-top: 7px}