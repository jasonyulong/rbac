    /**
     * @desc 获取岗位id
     * @author 杨能文
     * @date 2019/4/26 20:14
     * @access public
     * @param $jobArr
     * @return int
     */
    public function getJob($jobArr):int
    {
        $model = new UsersJob();
        $jobId = 0;
        $jobInfo = [];
        foreach($jobArr as $value){
            if($jobInfo){
                $map = ['title'=>$value, 'status'=>1, 'tid'=>$jobInfo['tid'], 'lid'=>['egt',$jobInfo['lid']], 'rid'=>['elt',$jobInfo['rid']] ];
            }else{
                $map = ['title'=>$value, 'status'=>1];
            }
            $info = $model->field('id,pid,tid,rid,lid')->where($map)->find();
            $info = is_object($info) ? $info->toArray() : [];
            if(!$info){
                $jobId = 0;
                break;
            }
            $jobInfo = $info;
            $jobId   = $info['id'];
        }
        return $jobId;
    }

    /**
     * @desc 更新所有下级用户数
     * @author 杨能文
     * @date 2019/4/26 20:48
     * @access public
     */
    public function updateUnder(){
        $model = new UsersJob();
        $service = new Position();
        $jobIdArr = $model->where(['rank'=>1,'status'=>1])->column('id');
        foreach($jobIdArr as $value){
            $service->updateunder($value);
        }
    }
