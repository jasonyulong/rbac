        $allJobArr    = array_flip($allJobArr);

        $addData = [];
        $i = 0;
        foreach($data as $key=>$val){
            $arr = [];
            if(!isset($userNameArr[$key])){
                $output->write("用户【{$key}】不存在\n");
                $i++;
                continue;
            }
            $arr['id'] = $userNameArr[$key];

            $jobInfo = explode('--',$val);
            //单个直接找
            if(count($jobInfo) == 1){
                $jobName = $jobInfo[0];
                if(!isset($allJobArr[$jobName])){
                    $output->write("用户【{$key}】--部门【{$jobName}】不存在\n");
                    $i++;
                    continue;
                }
                $arr['job_id'] = $allJobArr[$jobName];
            }

            //多层级
            if(count($jobInfo) > 1){
                $jobId = $this->getJob($jobInfo);
                if(!$jobId){
                    $output->write("用户【{$key}】--多岗位【{$val}】未找到\n");
                    $i++;
                    continue;
                }
                $arr['job_id'] = $jobId;
            }
            if($arr)array_push($addData,$arr);
        }

        $total = count($data);
        $success = count($addData);
        $usersModel->saveAll($addData);
        //更新所有下级用户数
        $this->updateUnder();
        $output->write("匹配用户:{$total}--匹配失败:{$i}--匹配成功:{$success}\n");
