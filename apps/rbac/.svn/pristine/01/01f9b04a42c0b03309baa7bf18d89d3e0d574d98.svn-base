{extend name="$config.layout" /}
{block name='content'}
<div class="dialog-content">
    <table class="table table-striped table-bordered table-hover">
        <tbody>
        {volist name="location" id="vo"}
        <tr>
            <td class="td-align td-padding col-md-6">{if condition="isset($vo[0])"}{$vo[0]}{/if}</td>
            <td class="td-align td-padding col-md-6">{if condition="isset($vo[1])"}{$vo[1]}{/if}</td>
        {/volist}
        </tbody>
    </table>
</div>
{/block}
