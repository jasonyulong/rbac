{extend name="$config.layout" /}
{block name='content'}
<div class="dialog-content">
    <table class="table table-striped table-bordered table-hover">
        <tbody>
        {volist name="label" id="vo"}
        {if condition="$vo"}
        <tr>
            <td class="td-align td-padding">销售标签</td>
            <td class="td-align td-padding">{$vo}</td>
        </tr>
        {/if}
        {/volist}
        </tbody>
    </table>
</div>
{/block}
